{
  "name": "ordinator",
  "displayName": "ordinator",
  "description": "order selection like a tree",
  "version": "0.0.1",
  "publisher": "Thiago P. Santos",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Formatters",
    "Other"
  ],
  "keywords": [
    "order",
    "format",
    "text"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "extension.orderSelection",
      "title": "Order Selection"
    }],
    "keybindings": [
      {
          "command": "extension.orderSelection",
          "key": "ctrl+alt+o",
          "mac": "cmd+alt+o",
          "when": "editorTextFocus"
      }
  ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.78.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0"
  }
}
